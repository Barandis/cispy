{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": [
      "src/"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "encoding": "utf8",
    "template": "node_modules/docdash",
    "destination": "docs/api/",
    "recurse": true,
    "verbose": true
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": false
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    }
  },
  "docdash": {
    "static": false,
    "sort": true
  }
}
